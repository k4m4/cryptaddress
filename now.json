{
    "name": "cryptaddress",
    "version": 2,
    "alias": [
      "cryptaddress.now.sh",
      "cryptocurrency-address-detector.now.sh"
    ],
    "builds": [
      { "src": "package.json", "use": "@now/next" },
      { "src": "index.js", "use": "@now/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
}